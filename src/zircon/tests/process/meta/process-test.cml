{
    include: [ "syslog/client.shard.cml" ],
    program: {
        runner: "elf",
        binary: "test/process-test",
        job_policy_create_raw_processes: "true",
        job_policy_ambient_mark_vmo_exec: "true",
        use_next_vdso: "true",
    },
    capabilities: [
        { protocol: "fuchsia.test.Suite" },
    ],
    expose: [
        {
            protocol: "fuchsia.test.Suite",
            from: "self",
        },
    ],
    facets: {
        "fuchsia.test": { type: "system" },
    },
}
